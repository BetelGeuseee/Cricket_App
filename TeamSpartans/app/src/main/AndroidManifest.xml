<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamspartans">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appiconcircle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appiconcircle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AllPlayer"></activity>
        <activity android:name=".EditingActivity" />
        <activity android:name=".SpartansList" />
        <activity android:name=".MyProfilePage" />
        <activity android:name=".ProfilePage" />
        <activity android:name=".RegisterPage" />
        <activity android:name=".LogInPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>